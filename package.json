{
  "name": "testapp2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "postversion": "react-native-version",
    "open:xcode": "open ios/testapp2.xcworkspace",
    "open:android-studio": "studio android",
    "ts": "tsc -p .",
    "tsw": "npm run ts -- --watch",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-reset-cache": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "android:run-dev": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant devDebug",
    "android:run-test": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant qaDebug",
    "android:run-stage": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant stageDebug",
    "android:run-prod": "node node_modules/react-native/local-cli/cli.js run-android --no-packager --variant prodDebug",
    "android:run+start": "node node_modules/react-native/local-cli/cli.js run-android --no-packager && node node_modules/react-native/local-cli/cli.js start",
    "android:build-release-bundle": "(cd android/ && ./gradlew bundleProdRelease)",
    "android:build-release-apk": "cd android && gradlew assembleProdRelease",
    "android:clean": "cd android && gradlew clean",
    "android:bundle": "node node_modules/react-native/local-cli/cli.js bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/ --dev false --sourcemap-sources-root=build",
    "android:bundle-visualizer": "react-native-bundle-visualizer --platform android",
    "ios:run": "node node_modules/react-native/local-cli/cli.js run-ios --no-packager",
    "ios:bundle-visualizer": "react-native-bundle-visualizer --platform ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "check-dependencies-updates": "npm outdated",
    "check-pods-updates": "cd ios && pod outdated",
    "environment:development": "SET ENVFILE=.env",
    "environment:test": "SET ENVFILE=.env.test",
    "environment:staging": "SET ENVFILE=.env.staging",
    "environment:production": "SET ENVFILE=.env.production",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch",
    "version:build": "react-native-version -b -A",
    "tests": "jest",
    "tests:watch": "jest --watchAll",
    "tests:coverage": "jest --coverage",
    "e2e:build-android-debug": "npx detox build --configuration android.emu.debug",
    "e2e:android-debug": "npx detox test --configuration android.emu.debug",
    "e2e:build-android-release": "npx detox build --configuration android.emu.release",
    "e2e:android-release": "npx detox test --configuration android.emu.release",
    "e2e:build-ios-debug": "npx detox build --configuration ios.emu.debug",
    "e2e:ios-debug": "npx detox test --configuration ios.emu.debug",
    "e2e:build-ios-release": "npx detox build --configuration ios.emu.release",
    "e2e:ios-release": "npx detox test --configuration ios.emu.release",
    "eslint:run": "eslint --ext=jsx,ts,tsx",
    "eslint:fix": "eslint --fix --ext=jsx,ts,tsx .",
    "generate-image-resources": "img-res-gen --dir=resources/images --out=src/common/ImageResources.g.ts --ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formatjs/intl-locale": "3.0.7",
    "@formatjs/intl-pluralrules": "5.1.4",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/datetimepicker": "6.7.0",
    "@react-native-community/netinfo": "9.3.6",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@reduxjs/toolkit": "1.9.0",
    "dayjs": "1.11.6",
    "i18next": "22.0.6",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "react": "18.1.0",
    "react-hook-form": "7.39.4",
    "react-i18next": "12.0.0",
    "react-native": "0.70.6",
    "react-native-android-keyboard-adjust": "1.2.0",
    "react-native-bootsplash": "4.3.3",
    "react-native-config": "1.4.11",
    "react-native-country-picker-modal": "2.0.0",
    "react-native-device-detection": "0.2.1",
    "react-native-device-info": "10.3.0",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-image-crop-picker": "0.38.1",
    "react-native-in-app-review": "4.2.1",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-localize": "2.2.4",
    "react-native-masked-text": "1.13.0",
    "react-native-modalize": "2.1.1",
    "react-native-navigation": "7.30.2",
    "react-native-navigation-hooks": "6.3.0",
    "react-native-orientation-locker": "1.5.0",
    "react-native-permissions": "3.6.1",
    "react-native-push-notification": "8.1.1",
    "react-native-share": "8.0.0",
    "react-native-snap-carousel": "4.0.0-beta.6",
    "react-native-svg": "13.6.0",
    "react-native-tab-view": "3.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "11.23.1",
    "react-redux": "8.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/runtime": "7.20.1",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.189",
    "@types/react": "18.0.25",
    "@types/react-native": "0.70.6",
    "@types/react-native-android-keyboard-adjust": "1.0.0",
    "@types/react-native-push-notification": "8.1.1",
    "@types/react-native-snap-carousel": "3.8.5",
    "@types/react-redux": "7.1.24",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux-logger": "3.0.9",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "babel-jest": "29.3.1",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "detox": "20.0.3",
    "eslint": "8.27.0",
    "eslint-config-react-strong": "2.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.5",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "lint-staged": "13.0.3",
    "patch-package": "6.5.0",
    "prettier": "2.7.1",
    "react-native-bundle-visualizer": "3.1.1",
    "react-native-config-node": "0.0.3",
    "react-native-dev-menu": "4.1.1",
    "react-native-image-resource-generator": "1.0.2",
    "react-native-version": "4.0.0",
    "react-test-renderer": "18.1.0",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm run ts && npm run eslint:run"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run ts",
      "npm run eslint:run"
    ]
  },
  "engines": {
    "npm": ">= 6.14.13",
    "yarn": "Please use npm >= 6.14.13"
  }
}
